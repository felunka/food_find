.card.mt-3
  %h5.card-header.d-flex.justify-content-between.align-items-center
    Token
    = button_to 'Add', registration_tokens_path, class: 'btn btn-primary', method: :post

  .card-body
    .row.row-cols-1.row-cols-md-3
      - @tokens.each do |token|
        = render partial: 'token_card', locals: {token: token}
